##### 5L User Preferences #####

# Defaults are in bold

# Format of 5L.db (ENCRYPTED, clear)
db_type = "clear"

# When should the 5L.db be written back to disk (write, close, EXIT)
db_writes = "write"

# FiveL application mode (window, FULLSCREEN)
mode = "fullscreen"

# Redoscript functionality (on, OFF)
redoscript = "on"

# Enable/Disable Debug Log (on, OFF)
debug_log = "on"

# Allow more than one instance of engine to launch simultaneously, default = no
multiple_instances = "yes"
